<p id="notice"><%= notice %></p>

<h1>Listing Pacientes</h1>

<table>
  <thead>
    <tr>
      <th>Identificacion</th>
      <th>Nombre</th>
      <th>Apellido</th>
      <th>Genero</th>
      <th>Direccion</th>
      <th>Estado</th>
      <th>Fechanacimiento</th>
      <th>Patologia</th>
      <th>Tipo documentos</th>
      <th>Estado civils</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @pacientes.each do |paciente| %>
      <tr>
        <td><%= paciente.identificacion %></td>
        <td><%= paciente.nombre %></td>
        <td><%= paciente.apellido %></td>
        <td><%= paciente.genero %></td>
        <td><%= paciente.direccion %></td>
        <td><%= paciente.estado %></td>
        <td><%= paciente.fechaNacimiento %></td>
        <td><%= paciente.patologia %></td>
        <td><%= paciente.tipo_documentos %></td>
        <td><%= paciente.estado_civils %></td>
        <td><%= link_to 'Show', paciente %></td>
        <td><%= link_to 'Edit', edit_paciente_path(paciente) %></td>
        <td><%= link_to 'Destroy', paciente, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Paciente', new_paciente_path %>
