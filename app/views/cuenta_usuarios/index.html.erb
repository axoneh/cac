<p id="notice"><%= notice %></p>

<h1>Listing Cuenta Usuarios</h1>

<table>
  <thead>
    <tr>
      <th>Identificacion</th>
      <th>Nombre</th>
      <th>Apellido</th>
      <th>Rol</th>
      <th>Genero</th>
      <th>Fechanacimiento</th>
      <th>Direccion</th>
      <th>Estado</th>
      <th>Tipo documentos</th>
      <th>Estado civils</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cuenta_usuarios.each do |cuenta_usuario| %>
      <tr>
        <td><%= cuenta_usuario.identificacion %></td>
        <td><%= cuenta_usuario.nombre %></td>
        <td><%= cuenta_usuario.apellido %></td>
        <td><%= cuenta_usuario.rol %></td>
        <td><%= cuenta_usuario.genero %></td>
        <td><%= cuenta_usuario.fechaNacimiento %></td>
        <td><%= cuenta_usuario.direccion %></td>
        <td><%= cuenta_usuario.estado %></td>
        <td><%= cuenta_usuario.tipo_documentos %></td>
        <td><%= cuenta_usuario.estado_civils %></td>
        <td><%= link_to 'Show', cuenta_usuario %></td>
        <td><%= link_to 'Edit', edit_cuenta_usuario_path(cuenta_usuario) %></td>
        <td><%= link_to 'Destroy', cuenta_usuario, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

