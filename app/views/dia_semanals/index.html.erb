<p id="notice"><%= notice %></p>

<h1>Listing Dia Semanals</h1>

<table>
  <thead>
    <tr>
      <th>Numero</th>
      <th>Nombre</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @dia_semanals.each do |dia_semanal| %>
      <tr>
        <td><%= dia_semanal.numero %></td>
        <td><%= dia_semanal.nombre %></td>
        <td><%= link_to 'Show', dia_semanal %></td>
        <td><%= link_to 'Edit', edit_dia_semanal_path(dia_semanal) %></td>
        <td><%= link_to 'Destroy', dia_semanal, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Dia semanal', new_dia_semanal_path %>
