<p id="notice"><%= notice %></p>

<h1>Listing Anticoagulantes</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Descripcion</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @anticoagulantes.each do |anticoagulante| %>
      <tr>
        <td><%= anticoagulante.nombre %></td>
        <td><%= anticoagulante.descripcion %></td>
        <td><%= link_to 'Show', anticoagulante %></td>
        <td><%= link_to 'Edit', edit_anticoagulante_path(anticoagulante) %></td>
        <td><%= link_to 'Destroy', anticoagulante, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Anticoagulante', new_anticoagulante_path %>
